; padding: 0px; overflow-wrap: break-word; }

.sidebar ul > li:last-child { margin-bottom: 0px; }

.sidebar .widget { padding: 0px 0px 20px; }

.header-right .widget { padding: 10px 0px; }

.sidebar .search-form { width: 100%; }

.footer-widgets { border-top: 1px solid rgb(245, 245, 245); padding: 40px 0=
px 16px; }

.footer-widgets .widget { margin-bottom: 24px; }

.footer-widgets .col-3 .widget-area { float: left; padding: 0px 3% 0px 0px;=
