.footer-widgets .col-4 .widget-area { float: left; padding: 0px 3% 0px 0px;=
 width: 25%; }

.footer-widgets .widget-area:last-child { padding: 0px; float: right; }

.site-footer { border-top: 1px solid rgb(229, 229, 229); padding: 40px 0px;=
